{
  "name": "timelogger",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "npm-run-all go:start react:start",
    "build": "npm-run-all react:build go:generate go:build",
    "test": "npm-run-all go:test react:test",
    "go:start": "go build -tags debug",
    "go:generate": "go generate",
    "go:build": "go build",
    "go:test": "go test ./...",
    "react:start": "BROWSER=./browser.js react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
